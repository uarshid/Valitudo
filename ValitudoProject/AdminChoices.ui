<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Frame" id="frame1">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="tk.Label" id="titleLabel">
        <property name="text" translatable="yes">Options</property>
        <layout manager="pack">
          <property name="pady">30</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="AddAPatient">
        <property name="text" translatable="yes">Add a Patient</property>
        <layout manager="pack">
          <property name="padx">30</property>
          <property name="pady">30</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="AddADoctor">
        <property name="text" translatable="yes">Add a Doctor</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="StatsButton">
        <property name="text" translatable="yes">View Database Stats</property>
        <layout manager="pack">
          <property name="pady">30</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="BackButton">
        <property name="text" translatable="yes">Go Back</property>
        <layout manager="pack">
          <property name="pady">20</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
